<div class='card'>
  <div class='footer'>
    <h3>{{name}}</h3>
    <p>{{description}}</p>
    <p><span class='specs'>When: </span>{{date}}</p>
    <p><span class='specs'>Where: </span>{{address}}</p>
    {{#each members}}
      <ul>
        <li>{{this.username}}</li>
      </ul>
    {{/each}}
  </div>
</div>